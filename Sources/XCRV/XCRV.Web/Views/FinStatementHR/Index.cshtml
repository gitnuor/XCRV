<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Customer</a></li>
                    <li class="breadcrumb-item active">Account Statement Search</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->
<!-- Main content -->
<div class="content">
    <div class="container-fluid">
        <div class="card card-default">
            <div class="card-header">
                <h3 class="card-title">Account Statement</h3>
                <div class="card-tools">
                    <button type="button" class="btn btn-tool" data-card-widget="collapse">
                        <i class="fas fa-minus"></i>
                    </button>
                </div>
            </div>
            
            <!-- /.card-header -->
            <div class="card-body">
                @*<form class="form-horizontal" asp-controller="FinStatementHR" asp-action="Index" >*@
                    <form  class="form-horizontal" asp-action="Index" asp-controller="FinStatementHR">
                    <div class="card-body">
                        <div class="row" style="margin-top: -25px;">

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>Account No</label>
                                    <div class='input-group'>
                                        @* <input type="text" id="txtFromDate" class="form-control singledate" />*@
                                        <input type="text" class="form-control" name="accno" id="txtSearchString" value="@ViewBag.Accno" />
                                        <input type="hidden" id="hdnAccountNo" name="hdnAccountNo" value="@ViewBag.AccountNo" />
                                        <input type="hidden" id="hdnFromDate" name="hdnFromDate" value="@ViewBag.FromDate" />
                                        <input type="hidden" id="hdnToDate" name="hdnToDate" value="@ViewBag.ToDate" />
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>From Date</label>
                                    <div class='input-group'>
                                        <input type="text" id="txtFromDate" name="FromDate" class="form-control singledate" />
                                        <div class="input-group-append">
                                            <label for="txtFromDate" class="input-group-text">
                                                <span class="fa fa-calendar"></span>
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>To Date</label>
                                    <div class='input-group'>
                                        <input type="text" id="txtToDate" name="ToDate" class="form-control singledate" />
                                        <div class="input-group-append">
                                            <label for="txtToDate" class="input-group-text">
                                                <span class="fa fa-calendar"></span>
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-3">
                                <div class="form-group">
                                    <label>&nbsp;</label>
                                    <div class='input-group'>
                                        <button id="btnSearch" type="button" class="btn btn-primary" value="Insert"><span id="btnText"> Search</span></button>
                                        &nbsp;&nbsp;
                                        <button id="btnShowPrint" type="button" class="btn btn-primary" value="Insert" style="visibility:hidden"><span id="btnShow"> Show Printable</span></button>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>


                    <div class="row">
                        <div class="col-md-6 grid-buttons-margin">
                            <div class="dt-buttons btn-group">
                                @* <button id="btnDownload" type="button" class="btn btn-primary"><span id="btnText"> Download Excel</span></button>*@
                                <button id="btnDownload" type="button" style="border:none;background:none"><img src="~/img/Exl.gif" alt="Excel" height="31px" /></button>
                                <img id="btnShowPdf" type="submit" src="~/img/pdf2.GIF" alt="pdf" height="31px" /> &nbsp;

                                <input type="hidden" id="hdnReport" name="hdnReport" value="@ViewBag.ReportData" />

                            </div>
                        </div>
                    </div>
                </form>


                <div class="row">
                    <div class="col-12" id="accordion">
                        <div class="card card-primary card-outline">
                            <a class="d-block w-100" data-toggle="collapse" href="#collapseOne">
                                <div class="card-header">
                                    <h4 class="card-title w-100">
                                        General
                                    </h4>
                                </div>
                            </a>
                            <div id="collapseOne" class="collapse show" data-parent="#accordion">
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-12">
                                            <table class="table table-striped">
                                                <tr style="line-height:9px;">
                                                    <th style=" width: 25%; text-align: right;font-size:14px;"><b>Account Name : </b></th>
                                                    <td style=" width: 25%; text-align: left;"> <label id="acct_name" class="font-weight-normal"> </label></td>
                                                    <th style=" width: 25%; text-align: right;font-size:14px;"><b>Customer Id : </b></th>
                                                    <td style=" width: 25%; text-align: left;"><label id="cust_id" class="font-weight-normal"> </label> </td>
                                                </tr>
                                                <tr style="line-height:9px;">
                                                    <th style=" width: 25%; text-align: right;font-size:14px;"><b>Account Type : </b></th>
                                                    <td style=" width: 25%; text-align: left;"> <label id="Account_Type" class="font-weight-normal"></label> </td>
                                                    <th style=" width: 25%; text-align: right;font-size:14px;"><b>Account No : </b></th>
                                                    <td style=" width: 25%; text-align: left;"><label id="foracid" class="font-weight-normal"> </label> </td>
                                                </tr>
                                                <tr style="line-height:15px;">
                                                    <th style=" width: 25%; text-align: right;font-size:14px;"><b>Address : </b></th>
                                                    <td style=" width: 25%; text-align: left;word-break:break-word!important;font-size:0.9em"><label id="address" class="font-weight-normal"> </label> </td>
                                                    <th style=" width: 25%; text-align: right;font-size:14px;"><b>Currency : </b></th>
                                                    <td style=" width: 25%; text-align: left;font-size:0.9em"><label id="acct_crncy_code" class="font-weight-normal"> </label> </td>
                                                </tr>
                                                <tr style="line-height:9px;">
                                                    <th style=" width: 25%; text-align: right;font-size:14px;"><b>Current Available Balance : </b></th>
                                                    <td style=" width: 25%; text-align: left;"> <label id="aval_balance" class="font-weight-normal" /></td>
                                                    <th style=" width: 25%; text-align: right;font-size:14px;visibility:hidden"><b>Current Effective Balance : </b></th>
                                                    <td style=" width: 25%; text-align: left;visibility:hidden"><label id="effect_balance" class="font-weight-normal"></label> </td>
                                                </tr>

                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                        <div class="card card-primary card-outline">
                            <a class="d-block w-100" data-toggle="collapse" href="#collapseTwo">
                                <div class="card-header">
