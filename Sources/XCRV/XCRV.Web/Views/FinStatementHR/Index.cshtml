                                    <div class="row">
                                        <div class="col-12">
                                            <table id="customerLimit" class="table table-bordered table-striped">
                                                <thead>
                                                    <tr style="line-height:9px;">
                                                        <th>Tran.ID</th>
                                                        <th>Date</th>
                                                        <th>Tran Particulars</th>
                                                        <th>Withdraw</th>
                                                        <th>Deposit</th>
                                                        <th>Balance</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- /.card-body -->
            <div class="card-footer">
            </div>
        </div>

    </div><!-- /.container-fluid -->
</div><!-- /.contant -->

<div class="modal fade" id="printableModal">
