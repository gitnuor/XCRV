@using XCRV.Domain.Entities;
@model IEnumerable<ChqStopReport>


<div class="col-12">
    <input type="hidden" id="errorMessage" value="@ViewBag.ErrorMessage" />
    <table class="table table-striped table-bordered smallfrontDataTable" id="chqLog">
        <thead>
            <tr>
                <th >Account.Number</th>
                <th >Account.Title</th>
                <th style="width: 10%;">Cheque.No</th>
                <th >Status</th>
                <th >Date</th>
                <th >Agent.ID</th>
                <th >Agent.Remarks</th>
                <th >Supervisor.Remarks</th>
                <th >Supervisor.ID</th>
                <th >Check.Date</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Account_Number</td>
                    <td>@item.Account_Title</td>
                    <td>@item.Cheque_No</td>
                    <td>@item.Cheque_Status</td>
                    <td>@item.Date_Time</td>
                    <td>@item.Agent_ID</td>
                    <td>@item.Agent_Remarks</td>
                    <td>@item.Supervisor_Remarks</td>
                    <td>@item.Supervisor_ID</td>
                    <td>@item.Check_Date</td>
                </tr>
            }
        </tbody>
    </table>
</div>