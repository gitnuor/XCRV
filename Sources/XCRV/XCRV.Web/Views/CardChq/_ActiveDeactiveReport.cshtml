@using XCRV.Domain.Entities;
@model IEnumerable<CardChqReport>


<div class="col-12">
    <input type="hidden" id="errorMessage" value="@ViewBag.ErrorMessage" />
    <table class="table table-striped table-bordered smallfrontDataTable" id="chqLog">
        <thead>
            <tr>
                <th>Card Number</th>
                <th>NRIC Numbe</th>
                <th style="width: 10%;">Card Holder Name</th>
                <th>Cheque Status</th>
                <th>Cheque No</th>
                <th>Date Time</th>
                <th>Agent ID</th>
                <th>Agent Remarks</th>
                <th>Supervisor Remarks</th>
                <th>Supervisor ID</th>
                <th>Checked Date Time</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>@item.cardNo</td>
                <td>@item.nric_number</td>
                <td>@item.cusName</td>
                <td>@item.chequeStatus</td>
                <td>@item.numChqNo</td>
                <td>@item.date_time</td>
                <td>@item.agentID</td>
                <td>@item.agentRemarks</td>
                <td>@item.superVisorRemarks</td>
                <td>@item.superVisorID</td>
                <td>@item.checkedDate</td>
            </tr>
            }
        </tbody>
    </table>
</div>