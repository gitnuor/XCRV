
@{
    ViewData["Title"] = ViewBag.Title;
    Layout = "~/Views/Shared/Template/_Layout.cshtml";
}

@model CaSaViewModel
<!-- Content Header (Page header) -->
<div class="content-header">
    <div class="container-fluid">
        <div class="row mb-2">
            <div class="col-sm-6">
                <h1 class="m-0"></h1>
            </div><!-- /.col -->
            <div class="col-sm-6">
                <ol class="breadcrumb float-sm-right">
                    <li class="breadcrumb-item"><a href="#">Deposit</a></li>
                    <li class="breadcrumb-item active">@ViewBag.Title</li>
                </ol>
            </div><!-- /.col -->
        </div><!-- /.row -->
    </div><!-- /.container-fluid -->
</div>
<!-- /.content-header -->

<div class="content">
    <div class="container-fluid">
        <div class="card card-default">
            <div class="card-header">
                <h3 class="card-title">@ViewBag.Title</h3>
                <div class="card-tools">
                    <button type="button" class="btn btn-tool" data-card-widget="collapse">
                        <i class="fas fa-minus"></i>
                    </button>
                </div>
            </div>
            <div class="card-body">
                <div class="row">
                    <div class="col-sm-12 col-md-8">
                        <form asp-controller="CaSaAccount" asp-action="Index">
                            <div class="input-group mb-3">
                                <div class="input-group-prepend">
                                    <button type="button" class="btn btn-warning">Account Number</button>
                                </div>
                                <!-- /btn-group -->
                                <input type="text" id="txtAccountNo" name="accountNo" class="form-control" value="@ViewBag.AccountNo">
                                <span class="input-group-append">
                                    <button type="submit" id="btnSearch" class="btn btn-info btn-flat">Search</button>
                                </span>
                            </div>
                        </form>
                    </div>
                    <div class="col-sm-12 col-md-4" style="padding-bottom: 20px;">
                        <div class="btn-group w-100">
                            <a class="btn btn-success " asp-controller="AccountTransaction" asp-action="Index" asp-route-accno="@ViewBag.AccountNo" target="_blank">
                                <span>Transaction</span>
                            </a>
                            <a class="btn btn-primary" asp-controller="FinStatement" asp-action="Index" asp-route-accno="@ViewBag.AccountNo" target="_blank">
                                <span>Statement</span>
                            </a>
                            <button type="button" class="btn btn-warning" id="btnNominee" name="btnNominee" >
                                <span>Nominee</span>
                            </button>

                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-12" id="accordion">
                        <div class="card card-primary card-outline">
                            <a class="d-block w-100" data-toggle="collapse" href="#collapseOne">
                                <div class="card-header">
                                    <h4 class="card-title w-100">
                                        1. General
                                    </h4>
                                </div>
                            </a>
                            <div id="collapseOne" class="collapse show" data-parent="#accordion">
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-12">
                                            <input type="hidden" id="hdnAccountNo" name="hdnAccountNo" value="@ViewBag.AccountNo" />
                                            <table class="table table-striped smallfrontTable">
                                                <tr>
                                                    <th style=" width: 25%; text-align: right;"><b>Account Name : </b></th>
                                                    <td style=" width: 25%; text-align: left;">@Model.CaSaAccount.Acct_Name</td>
                                                    <th style=" width: 25%; text-align: right;"><b>Balance : </b></th>
                                                    <td style=" width: 25%; text-align: left;">@Model.CaSaAccount.Clr_Bal_Amt</td>
                                                </tr>
                                                <tr>
                                                    <th style=" width: 25%; text-align: right;"><b>Customer ID : </b></th>
                                                    <td style=" width: 25%; text-align: left;"><i class="fa fa-user"></i><a href="/CustomerGeneralDetails/Index?customerId=@Model.CaSaAccount.Cust_Id"> @Model.CaSaAccount.Cust_Id </a></td>
                                                    <th style=" width: 25%; text-align: right;"><b>GL Subhead : </b></th>
                                                    <td style=" width: 25%; text-align: left;">@Model.CaSaAccount.Gl_Sub_Head_Code</td>
                                                </tr>
                                                <tr>
                                                    <th style=" width: 25%; text-align: right;"><b>Scheme Type : </b></th>
                                                    <td style=" width: 25%; text-align: left;">@Model.CaSaAccount.Schm_Type</td>
                                                    <th style=" width: 25%; text-align: right;"><b>Scheme Code : </b></th>
                                                    <td style=" width: 25%; text-align: left;">@Model.CaSaAccount.Schm_Code</td>
                                                </tr>
                                                <tr>
                                                    <th style=" width: 25%; text-align: right;"><b>A/C Status	: </b></th>
                                                    <td style=" width: 25%; text-align: left;">@Model.CaSaAccount.Acct_Status</td>
                                                    <th style=" width: 25%; text-align: right;"><b>A/C Status Date : </b></th>
                                                    <td style=" width: 25%; text-align: left;">@Model.CaSaAccount.Acct_Status_Date</td>
                                                </tr>
                                                <tr>
                                                    <th style=" width: 25%; text-align: right;"><b>PSO : </b></th>
                                                    <td style=" width: 25%; text-align: left;">@Model.CaSaAccount.Pso_Id</td>
                                                    <th style=" width: 25%; text-align: right;"><b>MPSO : </b></th>
                                                    <td style=" width: 25%; text-align: left;">@Model.CaSaAccount.Mpso_Id</td>
                                                </tr>
                                                <tr>
                                                    <th style=" width: 25%; text-align: right;"><b>RPSO : </b></th>
                                                    <td style=" width: 25%; text-align: left;">@Model.CaSaAccount.Rpso_Id</td>
